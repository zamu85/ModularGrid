<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core">
    <DataTemplate x:Key="colExamBlockTypeCellTemplate">
        <Image
            Width="16"
            Height="16"
            Source="{dx:DXImage 'Images/Maps/GeoPointMap_32x32.png'}"
            Stretch="Uniform" />
    </DataTemplate>

    <DataTemplate x:Key="examTemplate">
        <StackPanel
            HorizontalAlignment="Left"
            VerticalAlignment="Center"
            Orientation="Horizontal">
            <Image
                Width="32"
                Height="32"
                Source="{dx:DXImage 'Images/Maps/GeoPointMap_32x32.png'}" />

            <TextBlock Margin="20,10,0,0" Text="EEG exam" />
        </StackPanel>
    </DataTemplate>
</ResourceDictionary>
