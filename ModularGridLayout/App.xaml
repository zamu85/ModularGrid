<Application
    x:Class="ModularGridLayout.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="View/Styles/TableStyle.xaml" />
                <ResourceDictionary Source="View/Templates/PatientCellTemplate.xaml" />
                <ResourceDictionary Source="View/Templates/ExamCellTemplate.xaml" />
                <ResourceDictionary Source="View/Templates/FileCellTemplate.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
